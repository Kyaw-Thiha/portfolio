{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "checkJs": true,
    "allowJs": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["vite.config.ts"]
}
